<?xml version="1.0" encoding="utf-8"?>
<dynaForm name="userEdit.xml" basedir="" width="500" >
<abc type="title">
  <es>Editar Datos de Usuario</es>
  <en>Edit User Info</en>
  <po>Editar datos do user</po>
</abc>
<USR_FIRSTNAME type="text" size="25" maxlength="24" validate="Alpha" group="8">
  <es>Nombre:</es>
  <en>First Name:</en>
  <po>Nomes:</po>
</USR_FIRSTNAME>
<USR_MIDNAME type="text" size="25" maxlength="24" validate="Alpha">
  <es>Segundo Nombre:</es>
  <en>Middle Name:</en>
  <po>Segundo Nombre:</po>
</USR_MIDNAME>
<USR_LASTNAME type="text" size="25" maxlength="24" validate="Alpha">
  <es>Apellido:</es>
  <en>Last Name:</en>
  <po>Apellido:</po>
</USR_LASTNAME>
<USR_EMAIL type="text" size="25" maxlength="24" validate="Any">
  <es>Correo Electrónico:</es>
  <en>E-Mail:</en>
  <po>Correo Electronico:</po>
</USR_EMAIL>
<USR_USERNAME type="text" size="25" maxlength="32" validate="Alpha">
  <es>Identificador de Usuario:</es>
  <en>User Name:</en>
  <po>Nome do Usuario:</po>
</USR_USERNAME>
<USR_STATUS type="dropdown">
  <es>Estado Actual:
    <option name="ACTIVE"  >Activo</option>
    <option name="INACTIVE">Inactivo</option>
    <option name="VACATION">Vacación</option>
    <option name="CLOSE"   >Cerrado</option>
  </es>
  <en>Current Status:
    <option name="ACTIVE"  >Active</option>
    <option name="INACTIVE">Inactive</option>
    <option name="VACATION">Vacation</option>
    <option name="CLOSE"   >Close</option>
  </en>
  <po>Estado Actual:
    <option name="ACTIVE"  >Activo</option>
    <option name="INACTIVE">Inactivo</option>
    <option name="VACATION">Vacación</option>
    <option name="CLOSE"   >Cerrado</option>
  </po>
</USR_STATUS>
<USR_DUE_DATE type="date" mask="yyyy-mm-dd">
  <es>Fecha de Vencimiento:</es>
  <en>Due Date:</en>
  <po>Fecha de Caducidad:</po>
</USR_DUE_DATE>
<USR_USE_LDAP type="dropdown" >
  <es>Enable LDAP/AD
    <option name="Y">Enable</option>
    <option name="N">Disable</option>
  </es>
  <en>Enable LDAP/AD:
    <option name="Y">Enable</option>
    <option name="N">Disable</option>
  </en>
  <po>Enable LDAP/AD:
    <option name="Y">Enable</option>
    <option name="N">Disable</option>
  </po>
</USR_USE_LDAP>
<BSUBMIT type="submit">
  <es>Continuar</es>
  <en>Continue</en>
  <po>Continuar</po>
</BSUBMIT>
</dynaForm>