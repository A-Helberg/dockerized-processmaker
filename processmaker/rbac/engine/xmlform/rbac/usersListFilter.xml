<?xml version="1.0"?>
<dynaForm 
  cols="2"
  name="usersListFilter"
  width="100%"
  >

<UID type="text" dataCompareField="UID">
  <es>UID</es>
  <en>UID</en>
</UID>

<name type="text" dataCompareField="USR_USERNAME" dataCompareType="contains">
  <es>Nombre</es>
  <en>Name</en>
</name>

<email type="text" dataCompareField="USR_EMAIL" dataCompareType="contains">
  <es>eMail</es>
  <en>eMail</en>
</email>

<status type="dropdown" dataCompareField="USR_STATUS">
  <es>Estado Actual:
    <option name=""  ></option>
    <option name="ACTIVE"  >Activo</option>
    <option name="INACTIVE">Inactivo</option>
    <option name="VACATION">Vacación</option>
    <option name="CLOSE"   >Cerrado</option>
  </es>
  <en>Current Status:
    <option name=""  ></option>
    <option name="ACTIVE"  >Active</option>
    <option name="INACTIVE">Inactive</option>
    <option name="VACATION">Vacation</option>
    <option name="CLOSE"   >Close</option>
  </en>
  <po>Estado Actual:
    <option name=""  ></option>
    <option name="ACTIVE"  >Activo</option>
    <option name="INACTIVE">Inactivo</option>
    <option name="VACATION">Vacación</option>
    <option name="CLOSE"   >Cerrado</option>
  </po>
</status>

<search type="button" onclick="cGNXUnoxwrBvcDVYaTUzM1c2S2xuNnA3Wg______.doFilter( this.form )">
  <es>Filtrar</es>
  <en>Apply Filter</en>
</search>

</dynaForm>